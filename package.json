    "@types/leaflet.markercluster": "1.5.1",
    "@types/ms": "0.7.31",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "bcrypt": "5.0.1",
    "cookie": "0.5.0",
    "cross-env": "7.0.3",
    "cypress": "10.3.0",
    "dotenv": "16.0.1",
