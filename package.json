{
  "name": "platzmonster",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "dev:external": "vite dev --host 0.0.0.0",
    "dev:e2e": "cross-env DATABASE=platzmonster-e2e svelte-kit dev",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview --host 0.0.0.0 --port 6005",
